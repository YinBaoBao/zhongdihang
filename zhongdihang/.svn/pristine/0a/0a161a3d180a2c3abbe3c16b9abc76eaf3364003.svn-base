<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="renderer" content="webkit"/><!-- 让360浏览器默认选择webkit内核 -->

    <!-- 全局css -->
    <link rel="shortcut icon" href="/favicon.ico">
    <link href="/css/bootstrap.min.css?v=3.3.6" rel="stylesheet">
    <link href="/css/font-awesome.css?v=4.4.0" rel="stylesheet">
    <link href="/css/plugins/bootstrap-table/bootstrap-table.min.css" rel="stylesheet">
    <link href="/css/plugins/validate/bootstrapValidator.min.css" rel="stylesheet">
    <link href="/css/animate.css" rel="stylesheet">
    <link href="/css/style.css?v=4.1.0" rel="stylesheet">
    <link href="/css/_fstyle.css" rel="stylesheet">
    <link href="/css/plugins/iCheck/custom.css" rel="stylesheet">
    <link href="/css/plugins/webuploader/webuploader.css" rel="stylesheet">
    <link href="/css/plugins/ztree/zTreeStyle.css" rel="stylesheet">
    <link href="/css/plugins/jquery-treegrid/css/jquery.treegrid.css" rel="stylesheet"/>
    <!-- 全局js -->
    <script src="/js/jquery.min.js?v=2.1.4"></script>
    <script src="/js/bootstrap.min.js?v=3.3.6"></script>
    <script src="/js/plugins/ztree/jquery.ztree.all.min.js"></script>
    <script src="/js/plugins/bootstrap-table/bootstrap-table.min.js"></script>
    <script src="/js/plugins/validate/bootstrapValidator.min.js"></script>
    <script src="/js/plugins/validate/zh_CN.js"></script>
    <script src="/js/plugins/bootstrap-table/bootstrap-table-mobile.min.js"></script>
    <script src="/js/plugins/bootstrap-table/locale/bootstrap-table-zh-CN.min.js"></script>
    <script src="/js/plugins/jquery-treegrid/js/jquery.treegrid.min.js"></script>
    <script src="/js/plugins/jquery-treegrid/js/jquery.treegrid.bootstrap3.js"></script>
    <script src="/js/plugins/jquery-treegrid/extension/jquery.treegrid.extension.js"></script>
    <script src="/js/plugins/layer/layer.min.js"></script>
    <script src="/js/plugins/iCheck/icheck.min.js"></script>
    <script src="/js/plugins/layer/laydate/laydate.js"></script>
    <script src="/js/plugins/webuploader/webuploader.min.js"></script>
    <script src="/js/plugins/jquery-cookie/jquery.cookie.js"></script>
    <script src="/js/common/ajax-object.js"></script>
    <script src="/js/common/bootstrap-table-object.js"></script>
    <script src="/js/common/tree-table-object.js"></script>
    <script src="/js/common/web-upload-object.js"></script>
    <script src="/js/common/ztree-object.js"></script>
    <script src="/js/common/Bank.js"></script>
    <style>
        .form-group .control-label{
            text-align: center;
            padding-right: 8px;
        }
    </style>
</head>

<body class="gray-bg">
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="col-sm-8 col-sm-offset-2">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <a href="#" class="avatar" style="float: left;margin: -13px 8px 0 -4px;width: 40px;height: 40px;">
                    <img id="avatar" class="img-responsive img-circle" src="/img/user.png" alt="头像" underline="true" avatarImg="user.avatar" width="50px;" height="50px;"/>
                </a>
                <h5 style="margin-top: 4px;">用户管理</h5>
            </div>
            <div class="ibox-content">
                <div class="row row-lg">
                    <div class="col-sm-12">
                        <div class="ibox float-e-margins">
                            <div class="ibox-content" style="border:none !important; ">
                                <div class="form-horizontal" id="userInfoForm">
                                    <input type="hidden" id="id" value="user.id">
                                    <input type="hidden" id="sexValue" value="user.sex">
                                    <div class="row">
                                        <div class="col-sm-6 b-r">
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label">账户:</label>
                                                <div class="col-sm-8" style="padding-left: 0">
                                                    <input id="userName" class="form-control" name="账户" readonly="readonly"  :value="adminidata.orgName" style="border: 0;" >
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label">性别:</label>
                                                <div class="col-sm-8" style="padding-left: 0">
                                                    <select id="sex" class="form-control" name="性别"  :value="adminidata.sex">
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label">角色:</label>
                                                <div class="col-sm-8" style="padding-left: 0">
                                                    <input id="roleid" class="form-control" name="角色" underline="true" :value="adminidata.roles" disabled="disabled"/>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-sm-4 control-label">邮箱:</label>
                                                <div class="col-sm-8" style="padding-left: 0">
                                                    <input id="eMail" class="form-control" name="邮箱" type="email" :value="adminidata.eMail"/>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div id="driverInfoContent">
                                                <div class="form-group">
                                                    <label class="col-sm-4 control-label">姓名:</label>
                                                    <div class="col-sm-8" style="padding-left: 0">
                                                        <input id="realName" class="form-control" underline="true" name="姓名" :value="adminidata.realName"/>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-4 control-label">创建日期:</label>
                                                    <div class="col-sm-8" style="padding-left: 0">
                                                        <input id="birthday" class="form-control" name="创建日期" underline="true" type="text"
                                                               :value="adminidata.createTime | laydate"/>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-4 control-label">部门:</label>
                                                    <div class="col-sm-8" style="padding-left: 0">
                                                        <select id="userType" class="form-control" name="部门" readonly="readonly" :value="adminidata.userType">
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-4 control-label">电话:</label>
                                                    <div class="col-sm-8" style="padding-left: 0">
                                                        <input id="telephone" class="form-control" name="电话" :value="adminidata.telephone"/>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="progress progress-striped" id="progressTipArea" style="margin-top: 20px;">
                                        <div id="progressBar" style="width: 0%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="0" role="progressbar" class="progress-bar progress-bar-info">
                                        </div>
                                    </div>
                                    <div class="row btn-group-m-t">
                                        <div class="col-sm-10 text-right">
                                            <button class="btn btn-info" name="提交" id="ensure" icon="fa-check" onclick="UserInfoDlg.editSubmit()">提交</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</body>
<script src="/js/plugins/vue/vue.js"></script>
<script src="/modular/system/user/user_info.js"></script>
<script src="/js/content.js?v=1.0.0"></script>
</html>
